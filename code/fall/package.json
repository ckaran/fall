{
    "name": "fall-fall",
    "displayName": "fall-fall",
    "description": "bla",
    "license": "MIT",
    "repository": "not yet",
    "version": "0.0.1",
    "publisher": "matklad",
    "engines": {
        "vscode": "^1.18.0"
    },
    "devDependencies": {
        "neon-cli": "^0.1.22",
        "typescript": "^2.6.2",
        "vscode": "^1.1.10",
        "@types/node": "^6.0.40"
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "install": "neon build",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "main": "./out/src/main",
    "contributes": {
        "languages": [
            {
                "id": "fall",
                "extensions": [
                    ".fall"
                ]
            }
        ],
        "commands": [
            {
                "command": "fall-fall.semanticSelection",
                "title": "Semantic Selection"
            },
            {
                "command": "fall-fall.showSyntaxTree",
                "title": "Show Syntax Tree"
            },
            {
                "command": "fall-fall.metrics",
                "title": "Internal Metrics"
            }
        ],
        "keybindings": [
            {
                "command": "fall-fall.semanticSelection",
                "key": "shift+alt+right",
                "when": "editorTextFocus && editorLangId == fall"
            }
        ]
    },
    "activationEvents": [
        "onLanguage:fall"
    ]
}
